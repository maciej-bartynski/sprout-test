{
  "name": "deep",
  "version": "1.0.0",
  "description": "backend",
  "main": "index.js",
  "scripts": {
    "build:prod": "env IS_PRODUCTION='true' ROLLUP_OPTIONS='prod' rollup --config configs/rollup.config.js",
    "start:prod": "env IS_PRODUCTION='true' node build/build.js",
    "start:dev": "env IS_PRODUCTION='false' ROLLUP_OPTIONS='dev' rollup --config configs/rollup.config.js --watch",
    "build:seed": "env IS_PRODUCTION='false' ROLLUP_OPTIONS='seed' rollup --config configs/rollup.config.js",
    "start:seed": "env IS_PRODUCTION='false' node src/database/seed/runme.js",
    "lint": "eslint --config configs/eslint.config.js --ignore-path configs/.eslintignore src priv_modules",
    "pret": "prettier --config configs/prettier.config.js --write src/**/*.js priv_modules/**/*.js",
    
    "watch:server": "env IS_PRODUCTION='false' rollup --config configs/rollup.config.dev.js --watch",
    "build:server": "rollup --config configs/rollup.config.prod.js",
    
    "start:node": "env IS_PRODUCTION='false' node build/build.js",
    "lintr:src": "eslint --config configs/eslint.config.js --ignore-path configs/.eslintignore src priv_modules",
    "prttr:src": "prettier --config configs/prettier.config.js --write src/**/*.js priv_modules/**/*.js"
  },
  "keywords": [
    "backend",
    "express",
    "nodejs"
  ],
  "author": "maciej barty≈Ñski",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "devcert": "^1.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "uuid": "^7.0.3",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-run": "^2.0.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  }
}